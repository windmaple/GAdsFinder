{
  "manifest_version": 2,

  "name": "Adfinder",
  "description": "This extension tries to find a Google ad",
  "version": "1.0",

  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
